[run]
omit =
    ../torchopt/distributed/*
    ../torchopt/visual.py
    ../torchopt/version.py
    ../docs/*
    ../examples/*
    ../tutorials/*

[report]
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    if __name__ == ('__main__'|"__main__"):
    if TYPE_CHECKING:
